  //Add amenities modal
  #addAmenities.modal.fade(tabindex='-1' role='dialog' aria-labelledby='exampleModalCenterTitle' aria-hidden='true')
    .modal-dialog.modal-dialog-centered(role='document')
      .modal-content
        .modal-header
          h5#exampleModalLongTitle.modal-title Select Amenities
          button.close(type='button' data-dismiss='modal' aria-label='Close')
            span(aria-hidden='true') Ã—
        .modal-body
          div.amenitiesArray
            p.listingCard#parkingAmenity(onclick='amenity("parkingAmenity");') Parking
            p.listingCard#balconyAmenity(onclick='amenity("balconyAmenity");') Balcony
            p.listingCard#cabletvAmenity(onclick='amenity("cabletvAmenity");') Cable Tv
            p.listingCard#internetAmenity(onclick='amenity("internetAmenity");') Internet
            p.listingCard#cctvAmenity(onclick='amenity("cctvAmenity");') CCTV
            p.listingCard#elevetorAmenity(onclick='amenity("elevetorAmenity");') Elevetor 
            p.listingCard#waterAmenity(onclick='amenity("waterAmenity");') Water
          div(style='text-align: center; margin-top: 20px; margin-bottom: 20px;')
            .submitAmenities(onclick='saveAmenities()' data-dismiss='modal') Submit


